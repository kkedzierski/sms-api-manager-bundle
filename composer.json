{
  "name": "kkedzierski/sms-api-manager-bundle",
  "description": "Sms manager for sms api",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Kamil KÄ™dzierski",
      "email": "kamil.kedzierski@onet.com.pl"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "6.2.*",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/orm": "^2.13",
    "symfony/config": "^5.0 | ^6.0",
    "symfony/dependency-injection": "^5.0 | ^6.0",
    "symfony/http-kernel": "^5.0 | ^6.0",
    "symfony/smsapi-notifier": "6.1.*",
    "symfony/uid": "^6.2",
    "symfony/console": "^6.2",
    "symfony/flex": "^2.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpmd/phpmd": "^2.11",
    "phpstan/phpstan": "^1.2",
    "phpunit/phpunit": "^10.0",
    "phpro/grumphp": "^1.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "SmsApi\\SmsApiManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmsApi\\SmsApiManager\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true,
      "symfony/flex": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
