<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true" autoconfigure="true"/>
<!-- Logger -->
        <service
                id="Psr\Log\LoggerInterface"
                alias="logger"
                public="true"
        />
        <service
                id="logger"
                class="Psr\Log\NullLogger"
                public="false"
        />
<!-- Texter -->
        <service
                id="Symfony\Component\Notifier\Transport\TransportInterface"
                alias="texter.transport"
                public="true"
        />
        <service
                id="texter.transport"
                class="Symfony\Component\Notifier\Transport\Transports"
                public="false"
        >
            <argument
                    key="$transports"
                    type="collection"
                />
        </service>
        <service
                id="Symfony\Component\Notifier\TexterInterface"
                alias="texter"
                public="true"
        />
        <service
                id="texter"
                class="Symfony\Component\Notifier\Texter"
                public="false"
        >
            <argument
                    key="$transport"
                    type="service"
                    id="texter.transport"
            />
        </service>
        <service
                id="SmsApi\SmsApiManager\Service\SmsManagerInterface"
                alias="sms_api.sms_api_manager"
                public="true"
        />
        <service
                id="sms_api.sms_api_manager"
                class="SmsApi\SmsApiManager\Service\SmsManager"
                public="false"
        >
            <argument
                    key="$logger"
                    type="service"
                    id="logger"
            />
            <argument
                    key="$texter"
                    type="service"
                    id="texter"
            />
        </service>
    </services>
</container>
